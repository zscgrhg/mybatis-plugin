<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.eclipse.org/gemini/blueprint/schema/blueprint
			    http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd
			    http://www.springframework.org/schema/beans 
			    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			    http://www.springframework.org/schema/context 
			    http://www.springframework.org/schema/context/spring-context-3.0.xsd"
	default-autowire="byName">
	
	<osgi:service id="adminMapperOsgi" interface="com.tqlab.demo.dal.dao.AdminMapper" ref="adminMapper" auto-export="interfaces" />
	<osgi:service id="moviesMapperOsgi" interface="com.tqlab.demo.dal.dao.MoviesMapper" ref="moviesMapper" auto-export="interfaces" />
	<osgi:service id="starMapperOsgi" interface="com.tqlab.demo.dal.dao.StarMapper" ref="starMapper" auto-export="interfaces" />


</beans>
